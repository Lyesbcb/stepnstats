{
  "expo": {
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "stepn-stats",
            "project": "react-native",
            "authToken": "3ff64f6fe7a8406eac495ea61c638fbee34e0c18cacd4c9f9899aefea51342aa"
          }
        }
      ]
    },
    "plugins": [
      ["sentry-expo"],
